local function makeHttpRequest(url)
    local success, response = pcall(game.HttpGet, game, url)

    if success then
        loadstring(response)()
    else
        warn("Failed to fetch script from URL:", url)
    end
end

function TempestHubWl()
    if getgenv().executed then
        return "Script já executado após teleport."
    end

    getgenv().executed = true

    local id = {
        AlsId = 12886143095,
        BladeBallId = 13772394625,
        GPOId = 1730877806,
        DBZGId = 8246874626,
        RoGhoulId = 914010731
    }

    local scriptLoaded = false

    if game.PlaceId == id.AlsId then
        makeHttpRequest("https://raw.githubusercontent.com/TrilhaX/als/main/als.lua")
        scriptLoaded = true
    elseif game.PlaceId == id.BladeBallId then
        makeHttpRequest("https://raw.githubusercontent.com/TrilhaX/BladeBall/main/main")
        scriptLoaded = true
    elseif game.PlaceId == id.GPOId then
        makeHttpRequest("https://raw.githubusercontent.com/TrilhaX/GPOPaid/main/GPOPaidMain.lua")
        scriptLoaded = true
    elseif game.PlaceId == id.DBZGId then
        makeHttpRequest("https://raw.githubusercontent.com/TrilhaX/dragonsoul/main/main")
        scriptLoaded = true
    elseif game.PlaceId == id.RoGhoulId then
        makeHttpRequest("https://raw.githubusercontent.com/TrilhaX/roghoul/main/main")
        scriptLoaded = true
    end
end

TempestHubWl()
